{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"workspaces": {
		".": {
			"entry": [],
			"project": [],
			"ignoreBinaries": ["tsc"]
		},
		"packages/*": {
			"entry": ["src/**/*.test.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/config": {
			"project": ["src/**/*.ts"],
			"includeEntryExports": true
		}
	}
}
